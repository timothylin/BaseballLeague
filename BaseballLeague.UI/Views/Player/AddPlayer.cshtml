@model BaseballLeague.UI.Models.AddPlayerViewModel
@{
    ViewBag.Title = "AddPlayer";
}

<h2 class="padding-top-20">Add Player</h2>

<div>
    <div class="row">
        <div class="col-md-6">
            <br/>
            @using (Html.BeginForm("AddPlayer", "Player", FormMethod.Post))
            {
                <div class="container">
                <div class="row">
                    <div class="col-md-6">
                        <h6> Player Name</h6>
                        <p>@Html.TextBoxFor(m => m.player.PlayerName, new {style = "width: 100%"})</p>
                    </div>

                </div>
                </div>
            }
        </div>
    </div>
</div>




